{
  "name": "Spacecoin-Tokens",
  "version": "0.0.3",
  "description": "An application for interacting with the Spacecoin blockchain.",
  "repository": "spaceworksco/spacecoin-tokens",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "npm start devmode",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --macos --linux --windows",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "Spacecoin",
    "SPACE",
    "tokens",
    "NFT",
    "NFTs",
    "trading",
    "wallet",
    "inheritance",
    "heir",
    "cc",
    "GUI"
  ],
  "author": "SpaceWorks",
  "devDependencies": {
    "asymmetric-crypto": "^1.0.2",
    "bitcoinjs-lib": "^4.0.2",
    "bootstrap": "^4.1.3",
    "electron": "^7.2.4",
    "electron-builder": "22.6.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.4",
    "fix-path": "^2.1.0",
    "jquery": "^3.5.0",
    "popper.js": "^1.14.4",
    "webpack-cli": "^4.6.0"
  },
  "standard": {
    "globals": [
      "$"
    ]
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0"
  },
  "build": {
    "appId": "co.spaceworks.stargate",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel"
    }
  }
}
